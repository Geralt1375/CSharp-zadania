Zad 2.6

using System;

public class ProgramNext
{
	public static void Main()
	{
		
		int x, y = 4;
		x = (y -= 2);
		x = y++;
		x = y--; 

		Console.WriteLine(x);
		Console.WriteLine(y);
	 	
	}
}

Odpowiedz " x= 3, y= 2 "

Zad 2.7

public class ProgramNext3
{
	public static void Main()
	{
		
		int x, y = 5;
		x = ++y * 2;
		Console.WriteLine(x);
		Console.WriteLine(y);
		Console.WriteLine("-------------");
		x = y++;
		Console.WriteLine(x);
		Console.WriteLine(y);
		Console.WriteLine("-------------");
		x = y--;
		Console.WriteLine(x);
		Console.WriteLine(y);
		Console.WriteLine(++y); 
		Console.WriteLine("-------------");
	 	
	}
}

Odpowiedz : " wyswietli sie wartosc ++y, która wynosi 7 "

Zad 2.8 

public class ProgramNext4
{
	public static void Main()
	{
		
		bool x;
 		int y = 1, z = 1;
		 x = (y == 1 && z++ == 1);

		Console.WriteLine(x);
		Console.WriteLine(y);
		Console.WriteLine(z);
		Console.WriteLine("-------------");

	}
}

