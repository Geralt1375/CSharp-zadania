Zad 2.1
using System;
					
public class ProgramFahrenheit
{
	public static void Main()
	{
		
		double F, C;
		Console.WriteLine("Podaj temp. w stopniach Fahrenheita");
		C = double.Parse(Console.ReadLine());
		F = 32.0+9/5*C;
		Console.WriteLine(F);
		Console.ReadKey();

	}
}


Zad 2.2

public class ProgramDelta
{
	public static void Main()
	{
		
		
		double a, b, c, delta, delta2;
		Console.WriteLine("Podaj wspolczynniki a,b i c ");
		a = double.Parse(Console.ReadLine());
   	b = double.Parse(Console.ReadLine());
    c = double.Parse(Console.ReadLine());
		delta = b*b-4.0*a*c;
		Console.WriteLine(delta);
	  Console.ReadKey();
		
	
	}
}


Zad 2.3

public class ProgramBMI
{
	public static void Main()
	{
		
		
		double masa, wzrost, BMI;
		Console.WriteLine("Podaj mase i wage ");
		masa = double.Parse(Console.ReadLine());
   	wzrost = double.Parse(Console.ReadLine());
		BMI = masa/(wzrost*wzrost);
		Console.WriteLine(BMI);
	 	
		
	
	}
}


Zad 2.4

using System;

public class ProgramNext2
{
	public static void Main()
	{
		
		int x = 100;
    Console.WriteLine(++x * 2);

		Console.WriteLine(x);
		
	}
}

